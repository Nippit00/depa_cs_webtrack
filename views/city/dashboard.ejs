    #myChart {
      display: block;
      margin-left: 65%;
      margin-right: auto;
      margin-top: -500px;
      margin-bottom: 250px;
    }

    #spinner-container {
      display: none;
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(255, 255, 255, 0.9);
      z-index: 9999;
      overflow: hidden;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
    }

    #static-image {
      width: 300px;
