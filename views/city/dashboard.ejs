      height: auto;
      margin-bottom: 20px;
      /* ปรับตำแหน่งตามความเหมาะสม */
    }


    #spinner {
      width: 64px;
      height: 64px;
      border: 6px solid transparent;
      border-top-color: #3498db;
      border-radius: 50%;
      animation: spin 1s linear infinite;
      box-shadow: 0 0 0 1px rgba(0, 0, 0, 0.1);
    }

    @keyframes spin {
      to {
        transform: rotate(360deg);
      }
    }
